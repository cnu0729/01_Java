package pre.propertiesEx;

public class ProTest {
//필드	
	private String name;
	private int 평점;
	private int 관람객;
//메서드
	//기본생성자
	public ProTest() {
		// TODO Auto-generated constructor stub
	}
	//필수생성자
public ProTest(String name, int 평점, int 관람객) {
	super();
	this.name = name;
	this.평점 = 평점;
	this.관람객 = 관람객;
}
	//gettersetter
public String getName() {
	return name;
}
public int get평점() {
	return 평점;
}
public int get관람객() {
	return 관람객;
}

}
